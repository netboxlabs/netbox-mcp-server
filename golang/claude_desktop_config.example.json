{
  "mcpServers": {
    "netbox": {
      "command": "/absolute/path/to/netbox-mcp-server",
      "env": {
        "NETBOX_URL": "https://netbox.example.com",
        "NETBOX_TOKEN": "your_netbox_api_token_here",
        "TRANSPORT": "stdio",
        "VERIFY_SSL": "true",
        "LOG_LEVEL": "INFO"
      }
    }
  }
}
